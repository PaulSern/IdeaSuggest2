<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BDBDBD"
    tools:context="assignment.ideasuggest2.foodselection">

    <ImageButton
        android:id="@+id/hawkercentrebut"
        android:layout_width="183dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/hawkercentre"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="4dp"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/sushibut"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/sushibut"
        android:layout_width="132dp"
        android:layout_height="61dp"
        app:srcCompat="@drawable/sushi"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.909"
        android:layout_marginBottom="-41dp"
        app:layout_constraintBottom_toTopOf="@+id/fastfoodbut" />

    <ImageButton
        android:id="@+id/fastfoodbut"
        android:layout_width="157dp"
        android:layout_height="63dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/fastfood"
        android:layout_marginBottom="-29dp"
        app:layout_constraintBottom_toTopOf="@+id/nasikandarbut" />

    <ImageButton
        android:id="@+id/nasikandarbut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.979"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/nasikandar"
        android:layout_marginBottom="-38dp"
        app:layout_constraintBottom_toTopOf="@+id/internationalfoodbut" />

    <ImageButton
        android:id="@+id/internationalfoodbut"
        android:layout_width="201dp"
        android:layout_height="73dp"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.065"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        app:srcCompat="@drawable/internationalfood"
        tools:layout_editor_absoluteY="223dp" />

</android.support.constraint.ConstraintLayout>
